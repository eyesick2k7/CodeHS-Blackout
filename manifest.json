{
    "manifest_version": 3,
    "name": "CodeHS Blackout",
    "description": "Blacks out CodeHS's website",
    "version": "0.31.3",
    "icons": {
        "348": "icons/icon.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.codehs.com/*", "https://codehs.com/*"],
            "css": ["style.css"]
        }
    ],
    "action": {
        "default_popup": "index.html"
    }
}